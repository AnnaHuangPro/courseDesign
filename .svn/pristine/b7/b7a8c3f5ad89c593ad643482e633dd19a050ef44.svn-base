<div ng-controller="entityTipsDisplayController">
	<div class="row" ng-init="getDisplayInitData()">
		<form class="form-horizontal">
			<div class="row rowddd"></div>
			
		
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="EntityName">实体名 </label>
				<div class="col-md-8 value-display">
					<span>{{entityTips.entityName}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="EntityAttr">实体属性 </label>
				<div class="col-md-8 value-display">
					<span>{{entityTips.entityAttr}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<div class="col-md-offset-2 col-md-10">
					<input class="btn btn-primary col-md-1" value="返回"
						ng-click="cancelOperate()" />
				</div>
			</div>
		</form>
	</div>
</div>

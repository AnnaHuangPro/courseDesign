<form class="form-horizontal">
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <label>复制视图唯一编码</label>
        </div>
        <label class="col-sm-2 control-label" for="copyReportViewKey"></label>
        <div class="col-sm-8">
            <input type="text" class="form-control" id="copyReportViewKey"
                   ng-model='copyReportViewKey'>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input class="btn btn-primary col-md-2" value="确定"
                   ng-click="copyReportView()" />
        </div>
    </div>
</form>
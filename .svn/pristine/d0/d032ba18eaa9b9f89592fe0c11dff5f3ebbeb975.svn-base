<div ng-controller="permissionDisplayController">
	<div class="row" ng-init="getDisplayInitData()">
		<form class="col-md-11 form-horizontal">
			<div class="row rowddd"></div>
			<div class="form-group">
				<label class="col-md-2 control-label" for="name">名称</label>
				<div class="col-md-10">
					<span>{{permission.name}}</span>
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-2 control-label"for="code">编码</label>
				<div class="col-md-10">
					<span>{{permission.code}}</span>
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-2 control-label" for="module.name">对应模块</label>
				<div class="col-md-10">
					<span>{{permission.module.name}}</span>
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-offset-2 col-md-10">
					<a ui-sref="main.list.permission" class="btn btn-primary">取消</a>
				</div>
			</div>
		</form>
	</div>
</div>

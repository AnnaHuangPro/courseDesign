<div ng-controller="entityPropertyDefineDisplayController">
	<div class="row" ng-init="getDisplayInitData()">
		<form class="form-horizontal">
			<div class="row rowddd"></div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="entity">实体 </label>
				<div class="col-md-8 value-display">
					<span>{{entityPropertyDefine.entity}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="name">字段名 </label>
				<div class="col-md-8 value-display">
					<span>{{entityPropertyDefine.name}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="type">字段类型 </label>
				<div class="col-md-8 value-display">
					<span>{{entityPropertyDefine.type}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="DefaultValue">默认值 </label>
				<div class="col-md-8 value-display">
					<span>{{entityPropertyDefine.DefaultValue}}</span>
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-offset-2 col-md-10">
					<input class="btn btn-primary col-md-1" type="button" value="返回" ng-click="cancelOperate()" />
				</div>
			</div>
		</form>
	</div>
</div>

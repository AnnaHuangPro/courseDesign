  <div class="col-md-12">
  <div class="rowddd"></div>
	<form class="form-horizontal" role="form" name="form">
		<div class="form-group" ng-repeat="sqlParamName in sqlParamNames">
			<label class="col-md-4 control-label" >{{sqlParamName.name}}</label>
			<div class="col-md-8">
				<input type="text" class="form-control" ng-model="sqlParams[sqlParamName.name]">
			</div>
		</div>
		<div class="form-group">
			<div class="col-md-offset-2 col-md-10">
				<input class="btn btn-success col-md-2"  value="确定" ng-click='fillingReportSqlParams()'/> 
			</div>
		</div>
	</form>
</div>

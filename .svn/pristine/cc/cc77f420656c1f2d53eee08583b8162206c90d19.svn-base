<div ng-controller="permissionDisplayController">
	<div class="row" ng-init="getDisplayInitData()">
		<form class=" col-md-12 form-horizontal">
			<legend class="legend" data-toggle="collapse" href="">
				<span class="legend-title">基础信息</span>
			</legend>
			<div class="form-group col-md-6" for="code">
				<label class="col-md-4 control-label">名称</label>
				<div class="col-md-8 value-display">
					<span>{{permission.name}}</span>
				</div>
			</div>
			<div class="form-group col-md-6" for="code">
				<label class="col-md-4 control-label">编码</label>
				<div class="col-md-8 value-display">
					<span>{{permission.code}}</span>
				</div>
			</div>
			<div class="form-group col-md-6" for="code">
				<label class="col-md-4 control-label">对应模块</label>
				<div class="col-md-8 value-display">
					<span>{{permission.module.name}}</span>
				</div>
			</div>
			<div class="form-group col-md-12">
				<div class="col-md-offset-2 col-md-10">
					<input class="btn btn-sm" type="button" value="返回" ng-click="closeOperateTab()" />
				</div>
			</div>
		</form>
	</div>
</div>

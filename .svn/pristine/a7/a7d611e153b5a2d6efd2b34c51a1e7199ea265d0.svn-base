[menucontrol] {
    /* prevent user selection */
    -moz-user-select: -moz-none;
    -khtml-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
    user-select: none;

    /* default */
    font-family: Verdana, Helvetica, Arial, sans-serif;
    font-size:13px;
    color: #555;
    text-decoration: none;
}

[menucontrol] ul {
    margin: 0;
    padding: 0;
    list-style: none;
    border: none;
    overflow: hidden;
}

[menucontrol] li {
    position: relative;
    padding: 0 0 0 20px;
    line-height: 20px;
}

[menucontrol] li.menu-expanded i.menu-leaf-head, [menucontrol] li.menu-collapsed i.menu-leaf-head {display:none;}
[menucontrol] li.menu-expanded i.menu-branch-head, [menucontrol] li.menu-collapsed i.menu-branch-head {display:inline;}
[menucontrol] li.menu-leaf i.menu-branch-head {display:none;}
[menucontrol] li.menu-leaf i.menu-leaf-head {display:inline;}

[menucontrol] li i {
    cursor: pointer;
}

[menucontrol] li .menu-label {
    cursor: pointer;
    display: inline;
}


[menucontrol].menu-classic li.menu-expanded i.menu-branch-head {
    padding: 1px 10px;
    background: url("../images/folder.png") no-repeat;
}

[menucontrol].menu-classic li.menu-collapsed i.menu-branch-head {
    padding: 1px 10px;
    background: url("../images/folder-closed.png") no-repeat;
}

[menucontrol].menu-classic li.menu-leaf i.menu-leaf-head {
    padding: 1px 10px;
    background: url("../images/file.png") no-repeat;
}

[menucontrol].menu-classic li .menu-selected {
    background-color: #aaddff;
    font-weight: bold;
}


[menucontrol].menu-light li.menu-expanded i.menu-branch-head {
    padding: 1px 10px;
    background: url("../images/node-opened-2.png") no-repeat;
}

[menucontrol].menu-light li.menu-collapsed i.menu-branch-head {
    padding: 1px 10px;
    background: url("../images/node-closed-2.png") no-repeat;
}

[menucontrol].menu-light li.menu-leaf i.menu-leaf-head {
    padding: 1px 10px;
    width: 16px; height: 16px;
    background: none no-repeat;
}

[menucontrol].menu-light li .menu-selected {
    font-weight: bold;
}


[menucontrol].menu-dark li.menu-expanded i.menu-branch-head {
    padding: 1px 10px;
    background: url("../images/node-opened-light.png") no-repeat;
}

[menucontrol].menu-dark li.menu-collapsed i.menu-branch-head {
    padding: 1px 10px;
    background: url("../images/node-closed-light.png") no-repeat;
}

[menucontrol].menu-dark li.menu-leaf i.menu-leaf-head {
    padding: 1px 10px;
    width: 16px; height: 16px;
    background: none no-repeat;
}

[menucontrol].menu-dark li .menu-selected {
    font-weight: bold;
}

[menucontrol].menu-dark {
    color: #ddd;
}

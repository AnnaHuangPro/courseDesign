.CodeMirror {
  border: 1px solid #bbbbbb;
}
#mySidebar div {
  overflow-x: hidden;
}
#mySidebar div div {
  overflow-x: hidden;
}
#mySidebar div div div {
  overflow-y: hidden;
}
::-webkit-scrollbar {
  width: 9px;
  height: 0px;
}
::-webkit-scrollbar-button {
  border-radius: 0px 0px -5px -5px;
  background-color: #d8d8d8;
  width: 5px;
  height: 5px;
  inside: 5px;
}
::-webkit-scrollbar-track {
  -webkit-box-shadow: inset 0 0 6px #d8d8d8;
  -webkit-border-radius: 0px;
  border-radius: 0px;
  border: 1px solid #d8d8d8;
}
::-webkit-scrollbar-corner {
  background-color: #d8d8d8;
  width: 9px;
}
::-webkit-scrollbar-thumb {
  background-color: #40b2d8;
  border-radius: 5px;
}
::-webkit-resizer {
  width: 9px;
}

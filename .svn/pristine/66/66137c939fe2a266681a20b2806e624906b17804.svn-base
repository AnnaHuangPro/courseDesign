.DDataGridClass .ui-grid-viewport::-webkit-scrollbar-thumb {
  background-color: #40b2d8 !important;
}
.DDataGridClass .tree::-webkit-scrollbar-thumb {
  background-color: #40b2d8 !important;
}
.formBtnGroup.Right {
  position: absolute !important;
  right: 15px !important;
}
.required:after {
  content: "*";
  color: red;
  position: absolute;
  top: 8px;
  right: 4px;
}
.reference {
  background-image: url('/DDD3/ddd/ddd/extends/asset/login/icon_re.png');
}
div[uiView$="List"] .formBtnGroup {
  margin-right: 16px;
}
.navbar {

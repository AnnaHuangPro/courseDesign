.content {
  padding-top: 45px;
}
.content .menu {
  z-index: 2;
  overflow: auto;
  min-width: 200px;
  top: 0px;
  bottom: 0px;
  position: fixed;
}
.content #leftMenu .menu {
  background-color: #343434 !important;
  top: 45px !important;
  width: 200px !important;
}
.content #leftMenu .menu .w2ui-node-image > span {
  color: #ffffff !important;
}
.content #leftMenu .menu .w2ui-node-caption {
  color: #ffffff !important;
  font: 9.33333333px "Microsoft Yahei", "Hiragino Sans GB", Helvetica, "Helvetica Neue", "微软雅黑", Tahoma, Arial, sans-serif;
}
.content #leftMenu .menu .w2ui-expand {
  color: #ffffff !important;
}
.content #leftMenu .menu .w2ui-selected {
  background: #ffffff !important;
  border-left:8px solid #40b2d8!important;
}
.content #leftMenu .menu .w2ui-selected table {
  padding-left: 10px px !important;
}
.content #leftMenu .menu .w2ui-selected .w2ui-node-image > span {
  color: #000000 !important;
}
.content #leftMenu .menu .w2ui-selected .w2ui-node-caption {
  color: #000000 !important;
}
.content #leftMenu .menu .w2ui-selected .w2ui-expand {
  color: #000000 !important;
}
.content .mainContent {
  margin-left: 200px;
  padding-left: 15px;
  padding-right: 15px;
}
.content .mainContent #content a,
.content .mainContent #content input,
.content .mainContent #content select {
  cursor: pointer;
  -webkit-box-shadow: none ;
  box-shadow: none ;
  outline: none ;
}
.content .mainContent #content a:focus,
.content .mainContent #content input:focus,
.content .mainContent #content select:focus {
  -webkit-box-shadow: none ;
  box-shadow: none ;
  outline: none ;
}
.content .mainContent #content > .tab-content {
  background-color: #ffffff;
  *zoom: 1;
}
.content .mainContent #content > .tab-content:before,
.content .mainContent #content > .tab-content:after {
  display: table;
  content: "";
  line-height: 0;
}
.content .mainContent #content > .tab-content:after {
  clear: both;
}
.content .mainContent #content > ul.nav-tabs {
  border-bottom: 2px solid #40b2d8;
  margin-top: 0px;
  margin-right: 0px;
  margin-bottom: 10px;
  margin-left: 0px;
}
.content .mainContent #content > ul.nav-tabs > li.active {
  border-bottom: 5px solid #40b2d8;
}
.content .mainContent #content > ul.nav-tabs > li a {
  border: none;
  padding-bottom: 5px;
  color: #000000;
}
.content .mainContent #content > ul.nav-tabs > li a:hover {
  background-color: white;
}
.content .mainContent #content > ul.nav-tabs > li:hover .removeBtn {
  display: inline;
  transition: all 1s;
  -moz-transition: all 1s;
  /* Firefox 4 */
  -webkit-transition: all 1s;
  /* Safari 和 Chrome */
  -o-transition: all 1s;
}
.content .mainContent #content > ul.nav-tabs > li:hover .removeBtn:hover {
  color: #C31504;
  transform: rotate(180deg);
  transform-origin:50% 50% 0;
  transition: all 1s;
  -moz-transition: all 1s;
  /* Firefox 4 */

  -webkit-transition: all 1s;
  /* Safari å’Œ Chrome */

  -o-transition: all 1s;
}
.content .mainContent #content > ul.nav-tabs > li .removeBtn {
 display: none;
  padding-bottom:0 !important;
  position: absolute;
  top: 0px;
  right: -15px;
  font-size: 10px;
  transition: all 1s;
  -moz-transition: all 1s;
  /* Firefox 4 */

  -webkit-transition: all 1s;
  /* Safari å’Œ Chrome */

  -o-transition: all 1s;
}
.content .mainContent .btn {
  color: #ffffff;
  background-color: #40b2d8;
  border-color: #40b2d8;
}
.content .mainContent .btn > .glyphicon {
  color: #ffffff;
}
.content .mainContent .glyphicon {
  color: #40b2d8;
}
.content .mainContent .input-group-btn .btn {
  background-color: white;
  color: #40b2d8;
}
.content .mainContent .input-group-btn .btn .glyphicon {
  color: #40b2d8 !important;
}
.content .mainContent .legend {
  font-size: 18px !important;
  line-height: 35px;
}
.content .mainContent .legend[data-toggle="collapse"] {
  cursor: pointer;
}
.content .mainContent .legend .legend-title {
  padding-left: 15px;
}
.content .mainContent .value-display {
  padding-top: 7px;
}
.mainContent .formBtnGroup .btn {
  margin: 0 1px;
  background-color: #40b2d8;
  color: #ffffff;
}
.mainContent .formBtnGroup .btn > .glyphicon {
  color: #ffffff;
}
.mainContent .formBtnGroup .btn[disabled=disabled] {
  background-color: #999999;
  border: 1px solid #808080;
}
.mainContent .formBtnGroup .btn .glyphicon {
  margin-right: 4px;
  font-size: 16px;
  vertical-align: text-bottom;
}
.mainContent .formBtnGroup.topRight {
  padding-left: 14px;
}

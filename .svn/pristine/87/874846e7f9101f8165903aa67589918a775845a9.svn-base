<div ng-controller="billCodeDisplayController">
	<div class="row" ng-init="getDisplayInitData()">
		<form class="form-horizontal">
			<div class="row rowddd"></div>
			
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="inputCode">输入码 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.inputCode}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="operatorCode">操作人编码 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.operatorCode}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="operateDate">操作日期 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.operateDate}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="orgId">单位主键 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.orgId}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="remark">备注 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.remark}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="billCodeTypeName">编码类型名称 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.billCodeTypeName}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="billCodeRule">编码规则 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.billCodeRule}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="variables">变量 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.variables}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="seqences">序列号变量 </label>
				<div class="col-md-8 value-display">
					<span>{{billCode.seqences}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<div class="col-md-offset-2 col-md-10">
					<input class="btn btn-primary col-md-1" value="返回"
						ng-click="cancelOperate()" />
				</div>
			</div>
		</form>
	</div>
</div>

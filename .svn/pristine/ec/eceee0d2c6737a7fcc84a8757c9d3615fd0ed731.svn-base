<div ng-controller="moduleDisplayController">
	<div class="row" ng-init="getDisplayInitData()">
		<form class="col-md-11 form-horizontal">
			<div class="row rowddd"></div>
			<div class="form-group col-md-12">
				<legend class="legend" data-toggle="collapse" href="">
					<span class="legend-title">基础信息</span>
				</legend>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="name">模块名称 </label>
				<div class="col-md-8 value-display">
					<span>{{module.name}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="url">路径</label>
				<div class="col-md-8 value-display">
					<span>{{module.url}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="route">路由</label>
				<div class="col-md-8 value-display">
					<span>{{module.route}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="iconClass">图标</label>
				<div class="col-md-8 value-display">
					<span>{{module.iconClass}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="moduleType">是否为系统模块</label>
				<div class="col-md-8 value-display">
					<span>{{module.moduleType}}</span>
				</div>
			</div>
			<div class="form-group col-md-6" ng-show="module.moduleType == '否'">
				<label class="col-md-4 control-label" for="parent.name">父模块
				</label>
				<div class="col-md-8 value-display">
					<span>{{module.parent.name}}</span>
				</div>
			</div>		
			<div class="form-group">
				<div class="col-md-offset-2 col-md-10">
					<input class="btn btn-primary col-md-1" type="button" value="返回" ng-click="cancelOperate()" />
				</div>
			</div>	
		</form>
	</div>
</div>

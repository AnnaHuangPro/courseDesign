.clearfix {
  *zoom: 1;
}
.clearfix:before,
.clearfix:after {
  display: table;
  content: "";
  line-height: 0;
}
.clearfix:after {
  clear: both;
}
.vip_top {
  font: 12px/1.5 Tahoma, Helvetica, Arial;
}
.vip_top a:hover {
  color: red;
}
.vip_top .top-menu {
  margin-right: 75px;
}
.vip_top .navbar-brand {
  height: 35px;
  padding-top: 10px;
}
.vip_top .navbar {
  min-height: 35px;
  height: 35px;
}
.vip_top .navbar-toggle {
  margin-top: 4px;
  margin-bottom: 4px;
}
.vip_top .navbar-nav {
  min-height: 35px;
  height: 35px;
}
.vip_top .navbar-nav li {
  margin-right: 25px;
}
.vip_top .navbar-nav li a {
  padding: 0;
  line-height: 35px;
}
.vip_top .navbar-nav .vipUserName:hover {
  cursor: text;
}
.vip_top .navbar-nav .vipTopCss:hover {
  cursor: pointer;
  color: red;
}
.vip_top .nav,
.vip_top .pagination,
.vip_top .carousel,
.vip_top .panel-title a {
  cursor: default;
}
.vip_left {
  background: #f4f4f4;
}
.vip_left .vip_user_info {
  padding-top: 15px;
}
.vip_left .vip_user_info .vip_mod_user_avatar:hover .account-avatar-edit {
  height: 17px;
}
.vip_left .vip_user_info .account-avatar-edit {
  background: rgba(64, 64, 64, 0.5) none repeat scroll 0 0;
  bottom: 0px;
  color: #ccc;
  display: block;
  height: 0;
  left: 0;
  overflow: hidden;
  position: absolute;
  text-align: center;
  transition: all 0.3s ease 0s;
  width: 100%;
}
.vip_left .vip_user_info .account-avatar-edit:hover {
  cursor: pointer;
}
.vip_left .vip_user_info .vip_avatar {
  display: block;
  position: relative;
  width: 120px;
  height: 120px;
}
.vip_left .vip_user_info .vip_info_list {
  margin: 10px 0;
}
.vip_left .vip_user_info .vip_info_list ul {
  padding: 0;
}
.vip_left .vip_user_info .vip_info_list ul li {
  list-style: none;
}
.vip_left .vip_user_info .vip_mod_user_avatar {
  background-color: #f4f1f1;
  overflow: hidden;
}
.vip_left .vip_user_info .show_userInfo {
  margin: 51px 0px 0px 0px;
  text-align: center;
}
.vip_left .vip_user_info .nick_name {
  font-size: 15px;
  color: #333;
  font-family: tahoma, "microsoft yahei";
}
.vip_left .vip_user_info .account_quit {
  padding: 0;
  text-align: right;
}
.vip_left .vip_user_info .account_quit a {
  color: #d30000;
  text-decoration: none;
}
.vip_left .vip_user_info .account_quit a:hover {
  cursor: pointer;
  text-decoration: underline;
}
.vip_left .side_box_a {
  margin: 35px 0;
}
.vip_left .side_box_a .vip_upgrade_btn {
  width: 100%;
  background-color: #c91a00;
  height: 55px;
  text-align: center;
  font-size: 20px;
  font-weight: bold;
  font-family: "microsoft yahei";
  -webkit-box-shadow: 5px 5px 5px rgba(174, 164, 164, 0.99);
  -moz-box-shadow: 5px 5px 5px rgba(174, 164, 164, 0.99);
  box-shadow: 5px 5px 5px rgba(174, 164, 164, 0.99);
}
.vip_left .side_box_a .vip_upgrade_btn:hover {
  opacity: 0.8;
  font-size: 21px;
}
.vip_left .vip_function_menu {
  padding-right: 0;
  padding-bottom: 40px;
  font-family: "microsoft yahei";
}
.vip_left .vip_function_menu a:hover {
  text-decoration: none;
}
.vip_left .vip_function_menu .panel {
  border: 0;
  background: transparent;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.vip_left .vip_function_menu .panel.in {
  background: white;
}
.vip_left .vip_function_menu .panel.in .menu_icon {
  color: #d30000;
}
.vip_left .vip_function_menu .panel .collapsing {
  background: white;
}
.vip_left .vip_function_menu .open {
  background: white;
}
.vip_left .vip_function_menu .panel-heading {
  height: 70px;
  font-family: "microsoft yahei";
  background: transparent;
}
.vip_left .vip_function_menu .panel-heading .panel-title {
  height: 100%;
  line-height: 70px;
}
.vip_left .vip_function_menu .panel-heading .menu_tit {
  top: -6px;
  position: relative;
  margin-left: 10px;
  font-family: "microsoft yahei";
  font-size: 18px;
}
.vip_left .vip_function_menu .panel-heading .menu_icon {
  font-size: 30px;
  color: #d1d1d1;
}
.vip_left .vip_function_menu .panel-heading:hover {
  cursor: pointer;
  background: white;
}
.vip_left .vip_function_menu .panel-heading:hover .menu_icon {
  color: #d30000;
}
.vip_left .vip_function_menu .list-group {
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.vip_left .vip_function_menu .list-group li {
  padding-left: 60px;
  font-size: 14px;
  display: block;
  height: 35px;
  line-height: 35px;
  padding-left: 55px;
}
.vip_left .vip_function_menu .list-group li:hover {
  cursor: pointer;
}
.vip_left .vip_function_menu .list-group li:hover a {
  color: #d30000;
}
.vip_left .vip_function_menu .list-group li a {
  color: #595959;
}
.vip_left .vip_function_menu .list-group li .in {
  color: #d30000;
}
.vip_welcome .item img {
  width: 800px;
  height: 450px;
}
.vip_welcome h3 a {
  text-decoration: none;
  box-shadow: -10px 0 10px #fafafa, 10px 0 10px #fcfcfc, 0 -10px 10px #fdfdfd, 0 10px 10px #fcfcfc;
}
.logo_up .ngdialog-close {
  position: relative !important;
}

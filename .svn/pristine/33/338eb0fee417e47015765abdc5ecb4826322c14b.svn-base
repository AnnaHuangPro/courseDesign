<div class="row">
<div class="col-md-12">
	<div class="btn-dcolumnfilter btn-group formBtnGroup">
		<dpagination class="pull-left" grid-name="modelFileListGrid"></dpagination>
		<button class="btn " ng-click="addModelFile()">
			<i class='glyphicon glyphicon-plus-sign'></i>新增
		</button>
		<button class="btn" ng-click="refreshGrid()">
			<i class='glyphicon glyphicon-refresh'></i>刷新
		</button>
	</div>
	<dcolumnfilter grid-name="modelFileListGrid" class="btn-dcolumnfilter pull-right" />
</div>
<div class="btn-group formBtnGroup pull-right gridBtnGroup"></div>
<ddatagrid 
	class="col-md-12"
	name="modelFileListGrid"
	id="modelFileListGrid"
	operation-columns='operationColumns'
	operation-bar-buttons='operationBarButtons' 
	list-view-key="modelFileList">
</ddatagrid>
</div>


<div ng-controller="modelFileDisplayController">
	<div class="row" ng-init="getDisplayInitData()">
		<form class="form-horizontal">
			<div class="row rowddd"></div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="modelKey">名称 </label>
				<div class="col-md-8 value-display">
					<span>{{modelFile.name}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="modelKey">唯一标识 </label>
				<div class="col-md-8 value-display">
					<span>{{modelFile.modelKey}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="type">类型 </label>
				<div class="col-md-8 value-display">
					<span>{{modelFile.type}}</span>
				</div>
			</div>
			<div class="form-group col-md-6">
				<label class="col-md-4 control-label" for="category">类别 </label>
				<div class="col-md-8 value-display">
					<span>{{modelFile.category}}</span>
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-offset-2 col-md-10">
					<input class="btn btn-primary col-md-1" type="button" value="返回" ng-click="closeOperateTab()" />
				</div>
			</div>
		</form>
	</div>
</div>

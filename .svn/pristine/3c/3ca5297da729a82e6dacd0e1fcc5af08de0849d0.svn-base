<div class="row">
	<div class="col-md-12">
		<div class="btn-group formBtnGroup">
		<dpagination class="pull-left" grid-name="modelFileListGrid"></dpagination>
			<a class="btn " ng-click="addModelFile()"
					ng-if="hasPermission('modelFile_add')">
				<i class='glyphicon glyphicon-plus-sign'></i>新增</a>
			<a class="btn" ng-click="refreshGrid()">
				<i class='glyphicon glyphicon-refresh'></i>刷新</a>
		</div>
		<dcolumnfilter class="pull-right" grid-name="modelFileListGrid"/>
	</div>
				<ddatagrid 
					class="col-md-12"
					name="modelFileListGrid"
					id="modelFileListGrid"
					operation-columns='operationColumns'
					operation-bar-buttons='operationBarButtons' 
					list-view-key="modelFileList">
				</ddatagrid>
</div>

